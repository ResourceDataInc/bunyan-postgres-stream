{
  "name": "bunyan-postgres-stream",
  "description": "A bunyan stream for PostgreSQL",
  "version": "1.5.3",
  "author": {
    "name": "Forrest Desjardins",
    "email": "desjardinsfg@gmail.com",
    "url": "github.com/fdesjardins"
  },
  "dependencies": {
